  family nam
    edit ECF_FILES '/ecf/ecfnets/scripts/nam_p3'
    edit COM '/gpfs/dell1/nco/ops/com'
    edit PROJ 'NAM-OPS'
    edit QUEUE 'prod'
    edit QUEUESHARED 'prod_shared'
    family tm00
      edit ECF_FILES '/ecf/ecfnets/scripts/nam_p3'
      edit TMMARK '00'
      family obsproc
        family dump
          task jnam_dump2
            time 13:15
          task jnam_dump
            time 13:15
          task jnam_dump_alert
            trigger jnam_dump_post:release_nam12_dump_alert
          task jnam_dump_post
            trigger jnam_dump == complete and jnam_dump2 == complete
            event 1 release_nam12_dump_alert
        endfamily
        family prep
          task jnam_prep
            trigger ../dump/jnam_dump == complete and ../dump/jnam_dump2 == complete
          task jnam_prep_post
            trigger ../../analysis == complete
        endfamily
      endfamily
      family analysis
        task jnam_analysis_prico
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete
        task jnam_analysis_firewx
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete
        task jnam_analysis_hawaii
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete
        task jnam_analysis_alaska
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete
        task jnam_analysis
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete and ../tcrelocate == complete
        task jnam_analysis_conus
          trigger ../obsproc/prep/jnam_prep == complete and ../prelim == complete and ../coldstart == complete
      endfamily
    endfamily
  endfamily

